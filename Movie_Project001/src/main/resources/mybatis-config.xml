<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-config.dtd">
  
  <configuration>
  	<settings>
  		<setting name="jdbcTypeForNull" value="NULL"/>
  	</settings>
  
	<typeAliases>
		<typeAlias type="com.Movie_Project.dto.MovieDto" alias="movie"/>	
		<typeAlias type="com.Movie_Project.dto.MemberDto" alias="member"/>
  		<typeAlias type="com.Movie_Project.dto.ReviewsDto" alias="review"/>
	</typeAliases>
	
	<mappers>
		<mapper resource="mapper/movieMapper.xml"/>
		<mapper resource="mapper/memberMapper.xml"/>
		<mapper resource="mapper/theaterMapper.xml"/>
	</mappers>  
  </configuration>
  
  